{
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/bbsfr-staging-storefront",
          "awslogs-region": "ap-southeast-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 3000
        }
      ],
      "command": [
        "yarn",
        "start"
      ],
      "cpu": 0,
      "environment": [],
      "mountPoints": [],
      "image": "${DOCKER_IMAGE}",
      "name": "bbsfr-staging-storefront-container"
    }
  ],
  "memory": "500",
  "family": "bbsfr-staging-storefront",
  "requiresCompatibilities": [
    "EC2"
  ],
  "cpu": "500"
}